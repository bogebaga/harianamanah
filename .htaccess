RewriteCond %{HTTP_REFERER} !^http://www.harianamanah.id$      [NC]
RewriteCond %{HTTP_REFERER} !^http://www.m.harianamanah.id/.*$      [NC]
RewriteCond %{HTTP_REFERER} !^http://www.m.harianamanah.id$      [NC]
RewriteCond %{HTTP_REFERER} !^https://harianamanah.id/.*$      [NC]
RewriteCond %{HTTP_REFERER} !^https://harianamanah.id$      [NC]
RewriteCond %{HTTP_REFERER} !^https://m.harianamanah.id/.*$      [NC]
RewriteCond %{HTTP_REFERER} !^https://m.harianamanah.id$      [NC]
RewriteCond %{HTTP_REFERER} !^https://www.harianamanah.id/.*$      [NC]
RewriteCond %{HTTP_REFERER} !^https://www.harianamanah.id$      [NC]
RewriteCond %{HTTP_REFERER} !^https://www.m.harianamanah.id/.*$      [NC]
RewriteCond %{HTTP_REFERER} !^https://www.m.harianamanah.id$      [NC]




